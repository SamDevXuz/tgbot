{
    "name": "samdevx/anime-bot",
    "description": "A modern Telegram Bot for managing anime and subscriptions.",
    "authors": [
        {
            "name": "SamDevX",
            "email": "samdevx@example.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/database": "^10.0",
        "vlucas/phpdotenv": "^5.5",
        "guzzlehttp/guzzle": "^7.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "platform": {
            "php": "8.1"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5",
        "mockery/mockery": "^1.6"
    }
}
